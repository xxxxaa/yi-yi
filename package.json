{
  "name": "yiyi",
  "version": "0.1.0",
  "description": "YiYi - AI-powered real estate assistant",
  "type": "module",
  "main": "dist/index.mjs",
  "bin": {
    "yiyi": "dist/cli/entry.mjs"
  },
  "scripts": {
    "build": "tsdown && pnpm --filter yiyi-web build",
    "dev": "tsx src/cli/entry.ts",
    "dev:gateway": "tsx src/gateway/server.ts",
    "dev:web": "pnpm --filter yiyi-web dev",
    "lint": "oxlint",
    "format": "oxfmt --check src/",
    "test": "vitest run --config vitest.config.ts",
    "check": "tsc --noEmit && pnpm lint && pnpm format"
  },
  "engines": {
    "node": ">=22.12.0"
  },
  "packageManager": "pnpm@10.23.0",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.74.0",
    "commander": "^13.1.0",
    "json5": "^2.2.3",
    "openai": "^6.18.0",
    "tslog": "^4.9.3",
    "ws": "^8.18.0",
    "zod": "^3.24.0"
  },
  "devDependencies": {
    "@types/node": "^22.15.0",
    "@types/ws": "^8.18.0",
    "@vitest/coverage-v8": "^3.1.0",
    "oxfmt": "^0.28.0",
    "oxlint": "^1.43.0",
    "tsdown": "^0.20.3",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^3.1.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  }
}
